{
  "name": "contributte/facebook",
  "description": "Easy-to-use Facebook login wrapper for [`Nette Framework`](https://github.com/nette/).",
  "keywords": [
    "facebook",
    "login",
    "php",
    "sdk",
    "nette",
    "oauth2"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/facebook",
  "authors": [
    {
      "name": "Milan Felix Šulc",
      "homepage": "https://f3l1x.io"
    },
    {
      "name": "Filip Šuška"
    }
  ],
  "repositories": [
    { "type": "git", "url": "https://github.com/holyfork/facebook-graph-sdk" }
  ],
  "require": {
    "php": ">=7.2",
    "facebook/graph-sdk": "dev-next",
    "nette/di": "^3.0.0",
    "nette/http": "^3.0.1"
  },
  "require-dev": {
    "ninjify/qa": "^0.13",
    "ninjify/nunjuck": "^0.4",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-nette": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Facebook\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.4-dev"
    }
  }
}
