{
  "name": "contributte/facebook",
  "description": "Easy-to-use Facebook login wrapper for [`Nette Framework`](https://github.com/nette/).",
  "keywords": ["facebook", "login", "php", "sdk", "nette", "oauth2"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/facebook",
  "authors": [
    {
      "name": "Milan Felix Šulc",
      "homepage": "https://f3l1x.io"
    },
    {
      "name": "Filip Šuška"
    }
  ],
  "require": {
    "php": ">=7.2 || >=8.0",
    "league/oauth2-facebook": "^2.0",
    "nette/di": "^4.0",
    "nette/http": "^4.0"
  },
  "require-dev": {
    "ninjify/qa": "^0.12",
    "ninjify/nunjuck": "^0.4",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-deprecation-rules": "^0.12",
    "phpstan/phpstan-nette": "^0.12",
    "phpstan/phpstan-strict-rules": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Facebook\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}
