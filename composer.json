{
  "name": "contributte/facebook",
  "description": "Easy-to-use Facebook login wrapper for [`Nette Framework`](https://github.com/nette/).",
  "keywords": ["facebook", "login", "php", "sdk", "nette", "oauth2"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/facebook",
  "authors": [
    {
      "name": "Milan Felix Šulc",
      "homepage": "https://f3l1x.io"
    },
    {
      "name": "Filip Šuška"
    }
  ],
  "require": {
    "php": "^7.2",
    "facebook/graph-sdk": "^5.6",
    "nette/di": "~3.0.0",
    "nette/http": "~3.0.1"
  },
  "require-dev": {
    "ninjify/nunjuck": "^0.2.0",
    "ninjify/qa": "^0.8.0",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "phpstan/phpstan-nette": "^0.11",
    "phpstan/phpstan-shim": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Facebook\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.4-dev"
    }
  }
}
